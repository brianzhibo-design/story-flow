# StoryFlow 前端环境变量配置
# 复制此文件为 .env.local 并填入实际值

# =================================
# API 配置
# =================================

# 后端 API 地址 (生产环境)
VITE_API_BASE_URL=https://api.storyflow.com/api/v1

# WebSocket 地址 (生产环境)
VITE_WS_URL=wss://api.storyflow.com/ws

# 开发代理目标 (仅开发环境使用)
# VITE_API_PROXY_TARGET=http://localhost:8000
# VITE_WS_PROXY_TARGET=ws://localhost:8000

# =================================
# 应用配置
# =================================

# 应用标题
VITE_APP_TITLE=StoryFlow

# 应用版本
VITE_APP_VERSION=1.0.0

# 是否启用调试模式
VITE_DEBUG=false

# =================================
# 第三方服务 (可选)
# =================================

# 百度统计 ID
# VITE_BAIDU_ANALYTICS_ID=

# Google Analytics ID
# VITE_GA_ID=

# Sentry DSN
# VITE_SENTRY_DSN=
